     code : |1-
      PROCEDURE WIN_ResponseForm()
      
      // Default return value of the window
      MaFenêtre..ValeurRenvoyée = Faux
      
      // If it's a new record
      SI Glossary..NouvelEnregistrement ALORS
      	
      	// Title of the window
      	MaFenêtre..Titre = <§$0015§>
      	// Positions the input on the title
      	ecranPremier(EDT_TITLE)
      	
      SINON // If it is a modification
      	
      	// Title of the window
      	MaFenêtre..Titre = <§$0017§>
      	// Positions the input on the text
      	EcranPremier(EDT_TXT)
      	
      FIN
      
      // RTF option hidden if not requested, and displays the "simple" input area
      EDT_TXT..RichEdit = vrai
      
      // Fills the controls
      FichierVersEcran()
      
      // Fills the treeview by programming to easily retrieve the identifier
      FillTreeView("", 0)
      
      LOCAL
      // If a parent category is specified
      SI Glossary.ParentID > 0 ALORS
      	// Finds the parent category in the treeview
      	soit sParent = ArbreCherche(TREE_ParentCategory, Glossary.ParentID)
      	// If a parent category is retrieved
      	SI sParent <> "" ALORS
      		// Expands and selects this category
      		ArbreDéroule(TREE_ParentCategory, sParent)
      		ArbreSelectPlus(TREE_ParentCategory, sParent)
      	FIN
      FIN
      
      // Manage the access errors to the database
      // The error messages returned by the database are displayed
      QUAND Exception
      	Erreur(<§$0018§>, ExceptionInfo(errMessage))
      	// Re-enables the exceptions
      	ExceptionActive()
      	// Resumes the process
      	RepriseSaisie()
      FIN
