   -
     name : CategoryDelete
     procedure_id : 1781159049743937870
     type_code : 14
     code : |1-
      // Summary: Deletes a category and its entire tree structure
      // Syntax:
      //[ <Result> = ] CategoryDelete (<nGlossaryID> is int)
      //
      // Parameters:
      //	nGlossaryID (integer): Category identifier
      // Return value:
      // 	boolean: True if the deletion is effective, False otherwise
      //
      fonction CategoryDelete(LOCAL nGlossaryID est un entier) : booleen
      
      nIDGlossaryPosition est un entier
      
      // Finds the requested glossary
      hlitrecherche(Glossary, GlossaryID, nGlossaryID)
      si htrouve(Glossary) alors
      	
      	// Stores the position 
      	nIDGlossaryPosition = hsauveposition(Glossary)
      	
      	// Browses the child categories and answers
      	pour tout Glossary avec ParentID = nGlossaryID
      		// Starts the delete procedure
      		CategoryDelete(Glossary.GlossaryID)
      	FIN
      	
      	// Return to the position
      	HRetourPosition(nIDGlossaryPosition)
      	// Deletes the record
      	hsupprime(Glossary)
      sinon
      	
      	// If the glossary is not found, returns False
      	renvoyer faux
      fin
      
      
      // The deletion is effective
      renvoyer vrai
