      // Summary: Starts the search
      // Syntax:
      //Find ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //// Automatic procedure:
      // The procedure is run manually, during a call in the code, with a delay set to 50 hundredths of a second
      // 
      
      PROCEDURE Find()
      
      // Clears the previous search results
      supprimetout(garrResultSearch)
      
      // Grays the Next button
      BTN_F3..etat = grise
      
      // Starts the search
      QRY_FindResponse.pSearch = EDT_Find
      hexecuterequete(QRY_FindResponse)
      // If no answer
      si hnbenr(QRY_FindResponse) = 0 ALORS
      	// Displays a message
      	toastaffiche("No answer corresponds to your search", toastcourt, cvmilieu, chcentre)
      	// Clears the static
      	STC_QueryInfo = ""
      	retour
      FIN
      
      // Browses the results to store the treeview branches in order
      pour TOUT QRY_FindResponse 
      	// Clears the branch
      	sBranch est une chaine = ""
      	
      	// Finds the branch corresponding to this result
      	soit sBranchFound est une chaine = ArbreCherche(TREE_GLOSSARY, QRY_FindResponse.GlossaryID)
      	
      	// The returned branch contains the caption as well as the identifier of the branch
      	// Store the set of captions only
      	pour toute chaine sOneLevel de sBranchFound separee par tab
      		sBranch += [TAB] + extraitchaine(sOneLevel, 1, esc)
      	FIN
      	
      	// Stores the branch
      	ajoute(garrResultSearch, sBranch)
      FIN
      
      // Sorts the array of results
      tableautrie(garrResultSearch, ttcroissant)
      
      // If more than one answer is found, enables the button for browsing results
      si hnbenr(QRY_FindResponse) > 1 alors BTN_F3..Etat = Actif
      
      // Starts from first subscript
      gnSearchSubscript = 0
      Search_Position()
