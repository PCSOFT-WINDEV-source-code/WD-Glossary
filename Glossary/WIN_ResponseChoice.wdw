       identifier : 0x28b7f2b8065db14e
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           internal_properties : CAAAAAgAAAAo19Sy+/VOqKNuM1aSJwSv65gJtaxVrJbBmz/jfTe2V08o/HldBcfP7voLUUXCN2SneAfnu3lJwMNcHomj/CfPJG/WAzbqx1y5Fc58WWz9UYrBwJmhadmnOylbSgDbwuX+pTcr0qlCyxjxGQ==
           code : |1+
            
            sBranchSelection est une chaîne // Current selection in the treeview
            nIdentifier est un entier // Identifier of the element
            sParentBranch est une chaine // Parent branch
            nParentIdentifier est un entier // Identifier of the parent
            
            // Retrieve the selection and check the type (leaf/node)
            sBranchSelection = ArbreSelect(TREE_GLOSSARY)
            // If a branch is selected
            si sBranchSelection <> "" ALORS
            		
            	// Retrieves the selection identifier
            	nIdentifier = ArbreRécupèreIdentifiant(TREE_GLOSSARY, sBranchSelection)
            	
            	// Reads the corresponding record
            	HLitRecherchePremier(Glossary, GlossaryID, nIdentifier)
            	SI HTrouve(Glossary) = Faux ALORS RETOUR
            	
            	// If it is a category
            	si Glossary.IsCategory ALORS
            		// Defines this category as parent
            		nParentIdentifier = Glossary.GlossaryID
            	SINON // If it is an answer
            		// Defines its parent as parent
            		nParentIdentifier = Glossary.ParentID		
            	FIN
            	
            FIN
            
            // New record
            HRAZ(Glossary)
            
            // Defines the parent
            Glossary.ParentID = nParentIdentifier
            
            // Opens the category form
            SI Ouvre(WIN_CategoryForm) = faux ALORS retour
            
            // Finds the parent branch
            sParentBranch = ArbreCherche(TREE_GLOSSARY, Glossary.ParentID)
            // Adds the new category
            ArbreAjoute(TREE_GLOSSARY, sParentBranch +[tab]+ Glossary.Caption, IMG_ClosedDirectory, IMG_OpenedDirectory, Glossary.GlossaryID)
            // Expands and selects the category
            ArbreDéroule(TREE_GLOSSARY, sParentBranch +[TAB]+ Glossary.Caption)
            ArbreSelectPlus(TREE_GLOSSARY, sParentBranch +[TAB]+ Glossary.Caption)
            
            // Display the selection
            DisplaySelection()
           type : 32
