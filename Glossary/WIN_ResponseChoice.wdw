     internal_properties : CAAAAAgAAACM9C0oRwlrvSDTnnRB9A3eFUArarWx64cUJHp2nRvsvc5+PgZfapJiouDz9EhzYae4GYeS/3VX5vc6HH5Csvjuj7NMKYItW6M8i3s7XRtR3eRXHFf3ei88zqNO7C/ICmvpyFc9tvmlXlCDfjlY0tnAVz2xeJavijE4pUTKIeFnnp13pzU2a5SBFbr83PG95P+vrpQJPKReBPHQZkA7miBwblX6qnpn
     procedure_id : 1781159049744068942
     type_code : 14
     code : |1-
      // Summary: Restore the previous selection in the glossary from the information saved in the registry when closing windows
      // Syntax:
      // RestoreGlossary ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      Procédure RestoreGlossary()
      
      sPreviousSelection est une chaine
      sRegistryKey est une chaine
      
      // Registry key associated with the project
      sRegistryKey = projetinfo(piregistre)
      
      // Reads the value of the registry key ("Last Entry")
      sPreviousSelection = registrelit(sRegistryKey, <§$000c§>)
      sPreviousSelection = remplace(sPreviousSelection, "##%##", TAB)
      
      // If there was a previous selection
      si sPreviousSelection <> "" alors
      	
      	// Selects and expands the node
      	arbrederoule(TREE_GLOSSARY, sPreviousSelection)
      	arbreselectplus(TREE_GLOSSARY, sPreviousSelection)
      	
      	// Runs the selection code of a treeview node
      	executetraitement(TREE_GLOSSARY, trtSélection)
      	
      FIN
