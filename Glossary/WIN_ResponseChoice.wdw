     code : |1+
      // Syntax:
      //WIN_ChooseAnswder ([<gsChemin> is string [, <gsSearch> is string [, <gbExpandAll> is boolean [, <gbRestoreGlossary> is boolean]]]])
      //
      // Parameters:
      //	gsPath (ANSI string - default value=""): Access path to data files (created if not found)
      //	gsSearch (ANSI string - default value=""): Sought answer (according to abbreviation)
      //	gbExpandAll (boolean - default value=1): True to expand all the branches
      //	gbRestoreGlossary (boolean - default value=1): True to reselect the previous glossary at each opening
      // Return value:
      // 	None
      
      procédure WIN_ResponseChoice(gsPath est une chaine = "", gsFind est une chaine = "", gbExpandAll est un booleen = vrai, gbRestoreGlossary est un booleen = vrai)
      
      // Default return value
      mafenetre..ValeurRenvoyée = ""
      
      garrResultSearch est un tableau de chaines // Array of search results
      gnSearchSubscript est un entier // Subscript of current result
      
      // Location of the data file (if a specific locatino is requested)
      si gsPath <> "" alors HChangeRep(Glossary, gsPath)
      HVérifieStructure(Glossary, hcompatible)
      HCréationSiInexistant(Glossary)
      
      // Load the TreeView from the existing categories and entries
      FillTreeView("", 0)
      
      // If everything must be expanded
      si gbExpandAll alors arbrederouletout(TREE_GLOSSARY)
      
      // If a search is requested
      si gsFind <> "" ALORS
      	// Assigns the search control
      	EDT_Find = gsFind
      	// Starts the search
      	ExécuteTraitement(EDT_Find, trtmodification)
      SINON
      	// Restore the previous selection in the glossary
      	si gbRestoreGlossary alors RestoreGlossary()
      FIN
