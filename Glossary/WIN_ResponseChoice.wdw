        p_codes :
         -
           internal_properties : CAAAAAgAAAAXxSI5rouHKLx35aBjJqpjLCSzLoyCMFVMJ0ppoxKoD6t1wFAFe3nNI1ICnNCGGEv0yiy5zgDdngiZDZHVyvCjj3ORloXcECmBaRsAXJwhAPJ7SQpoATL2eOnvCb10SustEwcqZCuMWMW79BPwP5B37B5nr2RDSmVaFbtOwlipDys1JoXbNfxDs85obnObgwztfvUIzG4gGNb2TTyCIQ==
           code : |1-
            
            sBranchSelection est une chaîne // Current selection in the treeview
            nIdentifier est un entier // Identifier of the element
            sCaption est une chaine // Initial caption
            
            
            // Retrieve the selection and check the type (leaf/node)
            sBranchSelection = ArbreSelect(TREE_GLOSSARY)
            // If a branch is selected
            SI sBranchSelection = "" ALORS RETOUR
            
            // Retrieves the selection identifier
            nIdentifier = ArbreRécupèreIdentifiant(TREE_GLOSSARY, sBranchSelection)
            
            // Reads the corresponding record
            HLitRecherchePremier(Glossary, GlossaryID, nIdentifier)
            SI HTrouve(Glossary) = Faux ALORS RETOUR
            
            // Stores the initial caption
            sCaption = Glossary.Caption
            
            // If it is a category
            SI Glossary.IsCategory ALORS
            	
            	// Confirm the deletion
            	SI Dialogue(ChaîneConstruit(<§$0008§>, Glossary.Caption), ["Yes -Delete", "No"], 1, 2, dlgIcôneQuestion) = 2 ALORS retour
            	
            	// Delete the category and its entire content
            	SI CategoryDelete(nIdentifier) ALORS
            		// Deletes the treeview node
            		ArbreSupprime(TREE_GLOSSARY, sBranchSelection)
            	FIN
            	
            SINON // If it is an answer
            	
            	// Defines the confirmation message (that depends on the type of node)
            	soit sonfirmationMessage = chaineconstruit("Do you want to delete the %1 response?", Glossary.Caption)
            	
            	// Confirm the deletion
            	SI Dialogue(sonfirmationMessage, ["Yes -Delete", "No"], 1, 2, dlgIcôneQuestion) = 2 ALORS RETOUR
            	
            	// Deletes the record
            	HSupprime(Glossary)
            	
            	// Deletes the treeview node
            	ArbreSupprime(TREE_GLOSSARY, sBranchSelection)
            	
            FIN
            
            // Display the selection
            DisplaySelection()
