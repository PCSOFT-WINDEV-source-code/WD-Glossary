      // Summary: Positions the search
      // Syntax:
      // Search_Position ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Search_Position()
      
      // If the array is not filled
      SI garrResultSearch..Occurrence = 0 ALORS RETOUR
      
      // Incremented search subscript
      gnSearchSubscript++
      // If the new subscript exceeds the array bounds
      SI gnSearchSubscript > garrResultSearch..Occurrence ALORS
      	// Reinitializes the subscript
      	gnSearchSubscript = 1
      	// If more than one answer is found, displays an informative toast
      	SI garrResultSearch..Occurrence <> 1 _ET_ EDT_Find <> "" alors
      		ToastSupprimeTout()
      		ToastAffiche("End of Search"+rc+"You have been repositioned on the first search result", toastcourt, cvmilieu, chcentre)
      	FIN
      FIN
      
      // Displays the search information
      STC_QueryInfo = chaineconstruit("%1 search result(s) - %2/%1", garrResultSearch..Occurrence, gnSearchSubscript)
      
      // Selects the branch
      ArbreSelectPlus(TREE_GLOSSARY, garrResultSearch[gnSearchSubscript])
      
      // Runs the selection process
      ExécuteTraitement(TREE_GLOSSARY, trtSélection)
